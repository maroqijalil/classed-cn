{
  "name": "@classed-component/cn-react",
  "description": "A component creation utility for React with class composition flexibility supported by clsx and tailwind-merge under the hood.",
  "version": "0.0.1",
  "keywords": [
    "clsx",
    "cn",
    "tailwind-merge",
    "classed",
    "classed component",
    "styled component"
  ],
  "author": "Maroqi Jalil",
  "license": "MIT",
  "homepage": "https://github.com/maroqijalil/classed-component/blob/master/packages/cn-react/README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/maroqijalil/classed-component"
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/cn-react.umd.js",
  "module": "./dist/cn-react.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "types": "./dist/index.d.ts",
    "import": "./dist/cn-react.es.js",
    "require": "./dist/cn-react.umd.js"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "publish-package": "turbo run build lint && changeset version && changeset publish",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "vite": "^5.0.8",
    "vite-plugin-dts": "^3.7.2"
  },
  "dependencies": {
    "@emotion/is-prop-valid": "^1.2.1",
    "clsx": "^2.1.0",
    "tailwind-merge": "^2.2.1"
  }
}
